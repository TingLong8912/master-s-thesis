}

@inbook{RN196,
   author = {Schuurman, Nadine},
   title = {Spatial Ontologies},
   booktitle = {International Encyclopedia of Human Geography (Second Edition)},
   editor = {Kobayashi, Audrey},
   publisher = {Elsevier},
   address = {Oxford},
   pages = {419-424},
   abstract = {Spatial ontologies refer to different classification and conceptual frameworks used to develop datasets describing spatial entities. They are a reflection of epistemology; different ontologies emerge to describe identical spatial entities based on diverging ways of knowing the world. Different points of view result in different spatial ontologies. Formalization is the process of translating ideas and spatial entities into computable form. Ontologies are the basis of the resultant circumscribed logical universe. One computing system can accommodate multiple ontologies—or points of view. Understanding ontologies is the key to accommodating difference within a computing environment. Spatial ontologies enable multiple stakeholders to represent their data with different scenarios and interpretations of the geographical world. Spatial ontologies are also a way of embedding context into geographic information system (GIS) and cartography. Each ontology is implicitly a point of view and enables the map user or reader to understand that different classifications are associated with different agenda.},
   keywords = {Context
Epistemology
Formalization
GIScience
Interoperability
Ontology},
