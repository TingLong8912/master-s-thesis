      \strng{namehash}{8c05425f0ead705e4eb05746523a31d3}
      \strng{fullhash}{124c7f3da09fbf075713d85d1304c3aa}
      \strng{fullhashraw}{124c7f3da09fbf075713d85d1304c3aa}
      \strng{bibnamehash}{124c7f3da09fbf075713d85d1304c3aa}
      \strng{authorbibnamehash}{124c7f3da09fbf075713d85d1304c3aa}
      \strng{authornamehash}{8c05425f0ead705e4eb05746523a31d3}
