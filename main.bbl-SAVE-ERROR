      \strng{namehash}{d830ebe46291551d93b80a9a20b46713}
      \strng{fullhash}{c5e4be0fbf2ab4323920cfef8c0273cb}
      \strng{fullhashraw}{c5e4be0fbf2ab4323920cfef8c0273cb}
      \strng{bibnamehash}{c5e4be0fbf2ab4323920cfef8c0273cb}
      \strng{authorbibnamehash}{c5e4be0fbf2ab4323920cfef8c0273cb}
      \strng{authornamehash}{d830ebe46291551d93b80a9a20b46713}
      \strng{authorfullhash}{c5e4be0fbf2ab4323920cfef8c0273cb}
